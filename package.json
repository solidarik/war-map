{
  "name": "war-map",
  "version": "1.0.1",
  "description": "War moves",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": "19.x"
  },
  "scripts": {
    "test": "mocha -c",
    "pytest": "cd test && pytest -s -q --color=yes",
    "build-minify": "parcel build --no-source-maps --no-content-hash public-src/index.js && cp dist/index.js public/public-src.js",
    "build": "parcel build --no-minify --no-source-maps --no-content-hash public-src/index.js && cp dist/index.js public/public-src.js",
    "watch": "nodemon -w public-src -x npm run build",
    "dev": "nodemon --es-module-specifier-resolution=node index.js index.js",
    "start": "node --es-module-specifier-resolution=node index.js",
    "pack": "webpack --config webpack.config.js",
    "pack-map": "concurrently --kill-others \"npm run build\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidarik/war-map.git"
  },
  "keywords": [
    "WWII",
    "warmap",
    "troops",
    "openlayer",
    "koa",
    "move"
  ],
  "author": "Ildar Gubaydullin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/solidarik/war-map/issues"
  },
  "homepage": "https://github.com/solidarik/war-map#readme",
